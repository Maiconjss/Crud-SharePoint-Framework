{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "register-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/register/RegisterWebPart.js",
          "manifest": "./src/webparts/register/RegisterWebPart.manifest.json"
        }
      ]
    },
    "create-project-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/createProject/CreateProjectWebPart.js",
          "manifest": "./src/webparts/createProject/CreateProjectWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "RegisterWebPartStrings": "lib/webparts/register/loc/{locale}.js",
    "CreateProjectWebPartStrings": "lib/webparts/createProject/loc/{locale}.js"
  }
}
